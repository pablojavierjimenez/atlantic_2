{
  "name": "atlantic",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --hmr",
    "build": "ng build",
    "test": "jest --config ./jest.config.js",
    "coverage": "jest --coverage",
    "test:jest": "jest --coverage --config ./jest.config.js",
    "test:watch": "jest -o --watch --config ./jest.config.js",
    "e2e": "jest --config puppeteer-jest-config.js",
    "lint": "ng lint",
    "docs:json": "compodoc -p ./tsconfig.base.json -e json -d .",
    "storybook": "npm run docs:json && start-storybook -p 6006",
    "build-storybook": "npm run docs:json && build-storybook"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.2.4",
    "@angular/common": "~11.2.4",
    "@angular/compiler": "~11.2.4",
    "@angular/core": "~11.2.4",
    "@angular/forms": "~11.2.4",
    "@angular/platform-browser": "~11.2.4",
    "@angular/platform-browser-dynamic": "~11.2.4",
    "@angular/router": "~11.2.4",
    "rxjs": "~6.6.0",
    "stylelint-selector-bem-pattern": "^2.1.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.3"
  },
  "devDependencies": {
    "@angular-builders/jest": "^11.1.0",
    "@angular-devkit/build-angular": "~0.1102.3",
    "@angular/cli": "~11.2.3",
    "@angular/compiler-cli": "~11.2.4",
    "@babel/core": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "@babel/preset-typescript": "^7.13.0",
    "@compodoc/compodoc": "^1.1.14",
    "@ngneat/spectator": "^7.1.0",
    "@storybook/addon-actions": "^6.1.21",
    "@storybook/addon-essentials": "^6.3.4",
    "@storybook/addon-links": "^6.1.21",
    "@storybook/angular": "^6.3.4",
    "@testing-library/angular": "^10.3.2",
    "@types/expect-puppeteer": "^4.4.5",
    "@types/jasmine": "~3.6.0",
    "@types/jest": "^26.0.20",
    "@types/jest-environment-puppeteer": "^4.4.1",
    "@types/node": "^12.11.1",
    "@types/puppeteer": "^5.4.3",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "jest": "^26.6.3",
    "jest-preset-angular": "^8.4.0",
    "jest-puppeteer": "^4.4.0",
    "karma-coverage": "~2.0.3",
    "protractor": "~7.0.0",
    "puppeteer": "^8.0.0",
    "stylelint": "^13.13.1",
    "stylelint-config-idiomatic-order": "^8.1.0",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-sass-guidelines": "^8.0.0",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.19.0",
    "ts-jest": "^26.5.3",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.1.5"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "<rootDir>/Jest-setup.ts"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/dist/",
      "<rootDir>/e2e/src/",
      "<rootDir>/src/test.ts"
    ],
    "globals": {
      "ts-jest": {
        "tsConfig": "<rootDir>/tsconfig.spec.json",
        "stringifyContentPathRegex": "\\.html$"
      }
    }
  }
}
